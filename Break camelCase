// complete the function
function solution(string) {
    let arr = string.split('');
     for (let index = 0; index < arr.length; index ++){
        if (arr[index]=== arr[index].toUpperCase()){
        arr.splice(index, 0, " ")
        index++
      }
     } 
  return arr.join('');
}

solution("camelCasing")
