//Return the string in the description with the number of flies eaten for each frog.
function frogContest(n) {
  const gauss = num => num * (num + 1) /2
  const chris = gauss(n)
  const tom = gauss(Math.floor(chris/2))
  const cate = gauss(chris + tom)

  return `Chris ate ${chris} flies, Tom ate ${tom} flies and Cat ate ${cate} flies`
  
}

let x = frogContest(5)
