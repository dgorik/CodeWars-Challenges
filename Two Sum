/**
 * @param {number[]} nums
 * @param {number} target
 * @return {number[]}
 */
var twoSum = function(nums, target) {
    let answer = []
    for( i = 0; i <= nums.length; i++){
        if(nums[i] < target){
            for(j = i + 1; j <= nums.length - 1; j++){
                if(nums[i] + nums[j] === target){
                    answer.push(i)
                    answer.push(j)
                    return answer
                }
            }
        }
        
    }

};
