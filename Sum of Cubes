function sumCubes(n) {
  // Initialize sum to 0
  let sum = 0;

  // Loop from 1 to n
  for (let i = 1; i <= n; i++) {
    // Add the cube of the current number to the sum
    sum += i**3;
    // Log the intermediate sum (optional, for demonstration purposes)
    console.log(sum);
  }

  // Return the final sum
  return sum;
}

// Example usage: Calculate the sum of cubes from 1 to 2
let x = sumCubes(2);
// Log the result (total sum)
console.log(x);
